<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.vacanta.SecondHandler">
   <children>
      <DatePicker fx:id="myDatePicker" layoutX="188.0" layoutY="39.0" onAction="#getDate" promptText="Select a date.." />
      <TableView fx:id="myTableView" layoutX="67.0" layoutY="74.0" prefHeight="317.0" prefWidth="459.0">
        <columns>
            <TableColumn fx:id="hotelIdColumn" prefWidth="82.0" text="hotelID" />
            <TableColumn fx:id="startDateColumn" prefWidth="130.0" text="startDate" />
            <TableColumn fx:id="endDateColumn" prefWidth="116.0" text="endDate" />
            <TableColumn fx:id="percentsColumn" prefWidth="130.0" text="percents" />
        </columns>
      </TableView>
   </children>
</AnchorPane>
